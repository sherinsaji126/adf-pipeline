{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-sherin"
		},
		"azuredbsherin_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'azuredbsherin'"
		},
		"sherindb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sherindb'"
		},
		"BlobStorageforproduct_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageforproduct'"
		},
		"azuredbsherin_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "db-server-sherin.database.windows.net"
		},
		"azuredbsherin_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "new-db-sherin"
		},
		"azuredbsherin_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Sherinsaji126"
		},
		"sherindb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "db-server-sherin.database.windows.net"
		},
		"sherindb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "new-db-sherin"
		},
		"sherindb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Sherinsaji126"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-sherin_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azuredbsherin_password": {
						"value": "[parameters('azuredbsherin_password')]"
					},
					"sherindb_password": {
						"value": "[parameters('sherindb_password')]"
					},
					"BlobStorageforproduct_connectionString": {
						"value": "[parameters('BlobStorageforproduct_connectionString')]"
					},
					"azuredbsherin_properties_typeProperties_server": {
						"value": "[parameters('azuredbsherin_properties_typeProperties_server')]"
					},
					"azuredbsherin_properties_typeProperties_database": {
						"value": "[parameters('azuredbsherin_properties_typeProperties_database')]"
					},
					"azuredbsherin_properties_typeProperties_userName": {
						"value": "[parameters('azuredbsherin_properties_typeProperties_userName')]"
					},
					"sherindb_properties_typeProperties_server": {
						"value": "[parameters('sherindb_properties_typeProperties_server')]"
					},
					"sherindb_properties_typeProperties_database": {
						"value": "[parameters('sherindb_properties_typeProperties_database')]"
					},
					"sherindb_properties_typeProperties_userName": {
						"value": "[parameters('sherindb_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}